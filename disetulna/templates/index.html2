$def with (dise_district,dise_block,dise_cluster,klp_district,klp_block,klp_cluster)

<html>
<head>
  <title>DISE - KLP Matching - Karnataka Learning Partnership</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- Bootstrap -->
  <link href="lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen">
  <link href="css/app.css" rel="stylesheet" media="screen">

  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!--[if lt IE 9]>
<script src="../../assets/js/html5shiv.js"></script>
<script src="../../assets/js/respond.min.js"></script>
<![endif]-->
</head>
<body>
  <div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
          <li><a href="#">DISE Tulna</a></li>
        </ul>
        
        <p class="navbar-text">DISE</p>

        <form class="navbar-form navbar-left">
          <div class="form-group">
            <select class="form-control" id="klp_dist" name="dist" onchange="change_focus(this.value,'klp_blk',2);">
              <option value="0">District</option>
              $for dist in klp_district:
              <option value='$dist'>$dist.title()</option>
            </select>
          </div>
          <div class="form-group">
            <select class="form-control" id="klp_blk" name="blk" onchange="change_focus(this.value,'klp_clust',2);">
              <option value="0">Block</option>
            </select>
          </div>
          <div class="form-group">
            <select class="form-control" id="klp_clust" name="clust">
              <option value="0">Cluster</option>
            </select>
          </div>          
        </form>

        <form class="navbar-form navbar-left" role="search">
          <div class="form-group">
            <input type="text" class="form-control" placeholder="Search">
          </div>
          <!-- <button type="submit" class="btn btn-default">Submit</button> -->
        </form>

        <form class="navbar-form navbar-right">
          <div class="form-group">
            <select class="form-control" id="dise_dist" name="dist" onchange="change_focus(this.value,'dise_blk',2);">
              <option value="0">District</option>
              $for dist in dise_district:
              <option value='$dist'>$dist.title()</option>
            </select>
          </div>
          <div class="form-group">
            <select class="form-control" id="dise_blk" name="blk" onchange="change_focus(this.value,'dise_clust',2);">
              <option value="0">Block</option>
            </select>
          </div>
          <div class="form-group">
            <select class="form-control" id="dise_clust" name="clust" onchange="getdata();">
              <option value="0">Cluster</option>
            </select>
          </div>          
        </form>

        <p class="navbar-text pull-right">KLP</p>


      </div><!--/.navbar-collapse -->
    </div>
  </div>

  <script>
var dise_block=[
$for row in dise_block:
  ['$row[0]','$row[1]'],
['0','0']];

var dise_clust=[
$for row in dise_cluster:
  ['$row[0]','$row[1]'],
['0','0']];


var klp_block=[
$for row in klp_block:
  ['$row[0]','$row[1]'],
['0','0']];


var klp_clust=[
$for row in klp_cluster:
  ['$row[0]','$row[1]'],
['0','0']];

</script>
  <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
  <script src="//code.jquery.com/jquery.js"></script>
  <!-- Include all compiled plugins (below), or include individual files as needed -->
  <script src="lib/bootstrap/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="js/match.js"></script>

</body>
</html>